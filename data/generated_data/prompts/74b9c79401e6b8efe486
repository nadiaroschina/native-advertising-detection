        "item": "Билеты на выставку",
        "brand": "Biletsoff"
    },
    {
        "event": "Бизнес-конференция",
        "city": "Екатеринбург",
        "item": "Услуги по организации конференции",
        "brand": "EventPro"
    },
    {
        "event": "Кинофестиваль",
        "city": "Казань",
        "item": "Доступ к онлайн-показу",
        "brand": "OnlineCinema"
    },
    {
        "event": "Открытие нового ресторана",
        "city": "Нижний Новгород",
        "item": "Бронирование столиков",
        "brand": "RestHouse"
    },
    {
        "event": "Чемпионат по футболу",
        "city": "Самара",
        "item": "Сувенирная продукция",
        "brand": "SamaraTeamShop"
    },
    {
        "event": "Экологический фестиваль",
        "city": "Сочи",
        "item": "Проживание в эко-отеле",
        "brand": "GreenHouse"
    },
    {
        "event": "Запуск нового приложения",
        "city": "Тюмень",
        "item": "Скачать приложение",
        "brand": "AppStore"
    },
    {
        "event": "Фестиваль науки",
        "city": "Уфа",
        "item": "Книги по науке",
        "brand": "BookSpace"
    },
    {
        "event": "Концерт популярной группы",
        "city": "Хабаровск",
